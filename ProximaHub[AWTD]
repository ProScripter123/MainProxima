if game.CoreGui:FindFirstChild("TheChosenBad") then
  game.CoreGui.TheChosenBad:Destroy()
end
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/ProScripter123/Script/refs/heads/main/KavoUINameDisabler"))()

local Window = Library.CreateLib("Proxima Hub[AWTD]", "Ocean")
local Tab = Window:NewTab("OP")
local Section = Tab:NewSection("Use It Before Patched!")
local Section = Tab:NewSection("Inf Universe Material Gonna Kick You Need Rejoin")

local Tab2 = Window:NewTab("Credits")
local Section2 = Tab2:NewSection("Made By iEnz")
local Section2 = Tab2:NewSection("Credit To Howsly For Premium Battlepass")
