PreviousGold = nil
PreviousPuzzles = nil
PreviousLevelGui = nil
PreviousName = nil
PreviousLevel = nil
HideIdentity = nil

PreviousGold = game.Players.LocalPlayer.Data.Gold.Value
PreviousPuzzles = game.Players.LocalPlayer.Data.Puzzles.Value
PreviousLevelGui = game.Players.LocalPlayer.Data.Level.Value
PreviousName = game.Players.LocalPlayer.Character.Head.PlayerTag.Info.PlayerName.Text
PreviousLevel = game.Players.LocalPlayer.Character.Head.PlayerTag.Info.PlayerName.Text

Settings:NewButton("Hide Identity On", "Good For Scamming", function()

PreviousGold = game.Players.LocalPlayer.Data.Gold.Value
PreviousPuzzles = game.Players.LocalPlayer.Data.Puzzles.Value
PreviousLevelGui = game.Players.LocalPlayer.Data.Level.Value
PreviousName = game.Players.LocalPlayer.Character.Head.PlayerTag.Info.PlayerName.Text
PreviousLevel = game.Players.LocalPlayer.Character.Head.PlayerTag.Info.Rank_Level.Text
wait(1.5)
HideIdentity = true
while task.wait(0.5) do
if HideIdentity == true then
game.Players.LocalPlayer.Data.Gold.Value = 0 / 0
game.Players.LocalPlayer.Data.Puzzles.Value = 0 / 0
game.Players.LocalPlayer.Data.Level.Value = 0 / 0
game.Players.LocalPlayer.Character.Head.PlayerTag.Info.PlayerName.Text = "ProximaUser"
game.Players.LocalPlayer.Character.Head.PlayerTag.Info.Rank_Level.Text = "lvl:".. 0/0
end
end
end)

Settings:NewButton("Hide Identity Off", "Good For Scamming", function()
HideIdentity = false
wait(1.5)
game.Players.LocalPlayer.Data.Gold.Value = PreviousGold 
game.Players.LocalPlayer.Data.Puzzles.Value = PreviousPuzzles
game.Players.LocalPlayer.Data.Level.Value = PreviousLevelGui
game.Players.LocalPlayer.Character.Head.PlayerTag.Info.PlayerName.Text = PreviousName
game.Players.LocalPlayer.Character.Head.PlayerTag.Info.Rank_Level.Text = PreviousLevel 
end)
