if game.CoreGui:FindFirstChild("TheChosenBad") then
  game.CoreGui.TheChosenBad:Destroy()
end
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/ProScripter123/Script/refs/heads/main/KavoUINameDisabler"))()

local Window = Library.CreateLib("Proxima Hub[AWTD]", "Ocean")
local Tab = Window:NewTab("OP")
local Section = Tab:NewSection("Use It Before Patched!")

local Tab2 = Window:NewTab("Credits")
local Section2 = Tab2:NewSection("Made By iEnz")
local Section2 = Tab2:NewSection("Credit To Howsly For Premium Battlepass")
local Section2 = Tab2:NewSection("Credit To nkam For Inf Items Big Thanks To Him")
local Section2 = Tab2:NewSection("Proxima Hub Status : Not Patched")

local Tab3 = Window:NewTab("Inf Items")
local Section3 = Tab3:NewSection("You Gonna Get Kick And Rejoin And Do All Step For Do it")

local Auto = Window:NewTab("Auto")
local Auto = Auto:NewSection("Tell Me If Bug Still Beta")

Section:NewButton("Unlock ALL Banner(For Beginners)", "All banner will Visible To You", function()
while task.wait() do
print("Unlocked ALL Banner")
game.Players.LocalPlayer.PlayerGui.GachaUI.UI.BannerList.Banner2.Visible = true
game.Players.LocalPlayer.PlayerGui.GachaUI.UI.BannerList["Dream Banner"].Visible = true
game.Players.LocalPlayer.PlayerGui.GachaUI.UI.BannerList["Beginning Adventurers"].Visible = true
game.Players.LocalPlayer.PlayerGui.GachaUI.UI.BannerList.TimeUp.Visible = true
end
end)

Section:NewButton("Unlock Battlepass(Premium) And Max", "VERY OP STUFF", function()
if queue_on_teleport then
    queue_on_teleport('loadstring(game:HttpGet("https://raw.githubusercontent.com/buang5516/urban-octo-fishstick/main/supersecretscripts.lua"))()')
end

game:GetService("Players").LocalPlayer.Data.hasPass.Value = true
game:GetService("Players").LocalPlayer.Data.PassTier.Value = 60

for i = 1, 60 do
    task.spawn(function()
        local args = {
            [1] = i,
            [2] = "Free"
        }

        game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("ClaimRewardPass"):InvokeServer(unpack(args))

        local args2 = {
            [1] = i,
            [2] = "Premium"
        }

        game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("ClaimRewardPass"):InvokeServer(unpack(args2))
    end)
    task.wait()
end
end)

IsRolling = nil
Auto:NewButton("Auto Gacha 10x(Dream Banner)", "VERY OP STUFF", function()
print("Rolling ...")
IsRolling = true
while task.wait(0.5) do
if IsRolling == true then
local A_1 = 10
local A_2 = "Dream Banner"
local Event = game:GetService("ReplicatedStorage").Remote.Gacha
Event:InvokeServer(A_1, A_2)
end
end
end)

Auto:NewButton("Auto Gacha 10x(Beggining Adventures)", "VERY OP STUFF", function()
print("Rolling ...")
IsRolling = true
while task.wait(0.5) do
if IsRolling == true then
local A_1 = 10
local A_2 = "Beggining Adventures"
local Event = game:GetService("ReplicatedStorage").Remote.Gacha
Event:InvokeServer(A_1, A_2)
end
end
end)

Auto:NewButton("Auto Gacha 10x(Beyond Imagination)", "VERY OP STUFF", function()
print("Rolling ...")
IsRolling = true
while task.wait(0.5) do
if IsRolling == true then
local A_1 = 10
local A_2 = "Beyond Imagination"
local Event = game:GetService("ReplicatedStorage").Remote.Gacha
Event:InvokeServer(A_1, A_2)
end
end
end)

Auto:NewButton("Stop", "VERY OP STUFF", function()
print("Stopped")
IsRolling = false
end)


Section3:NewButton("Step 1", "Do All Step For Inf", function()
local items = {
    "Abyss Fragment", "Ado Medal", "Ancient Orb", "Ancient Shell", "Angry Ghost", "Arrancar Medal", "Blood Tear", "Broken Heavenly Spear"
}

for _, item in ipairs(items) do
    local args = {
        [1] = item,
        [2] = 0 / 0
    }
    game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Item"):WaitForChild("CraftItem"):InvokeServer(unpack(args))
end
end)

